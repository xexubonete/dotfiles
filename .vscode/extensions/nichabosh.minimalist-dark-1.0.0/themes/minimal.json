  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6A6A6A"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#90AED3"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.null",
        "variable.language.this",
        "support.type.primitive",
        "support.function",
        "support.variable.dom",
        "support.variable.property",
        "support.variable.property",
        "meta.property-name",
        "meta.property-value",
        "support.constant.handlebars"
      ],
      "settings": {
        "foreground": "#90AED3"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.modifier",
        "storage.type",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#9BD4B2"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "meta.function-call",
        "entity.other.attribute-name",
        "entity.name.function.shell"
      ],
      "settings": {
        "foreground": "#CEB0D3"
      }
    },
    {
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#9BD4B2"
      }
    }
  ]
}