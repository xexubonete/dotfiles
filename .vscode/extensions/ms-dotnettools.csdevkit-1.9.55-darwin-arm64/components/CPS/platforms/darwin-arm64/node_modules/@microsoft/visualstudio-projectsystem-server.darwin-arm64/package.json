{
  "name": "@microsoft/visualstudio-projectsystem-server.darwin-arm64",
  "version": "17.12.21-pre",
  "provides": {
    "projectSystems": [
      {
        "name": "CSProject",
        "extensions": [
          ".csproj"
        ],
        "icon": {
          "provider": "cpsProjectIconSourceService",
          "catalog": "ae27a6b0-e345-4288-96df-5eaf394ee369",
          "id": 758
        },
        "treeProvider": "cpsProjectTreeProvider"
      },
      {
        "name": "VBProject",
        "extensions": [
          ".vbproj"
        ],
        "icon": {
          "catalog": "ae27a6b0-e345-4288-96df-5eaf394ee369",
          "id": 3380
        }
      },
      {
        "name": "ShareProject",
        "extensions": [
          ".shproj"
        ],
        "icon": {
          "provider": "cpsProjectIconSourceService",
          "catalog": "ae27a6b0-e345-4288-96df-5eaf394ee369",
          "id": 2786
        },
        "treeProvider": "cpsProjectTreeProvider"
      }
    ],
    "projectTypes": [
      {
        "name": "UnitTestProject",
        "condition": "TestContainer"
      },
      {
        "name": "CSUnitTestProject",
        "condition": "CSharp & TestContainer",
        "icon": {
          "catalog": "ae27a6b0-e345-4288-96df-5eaf394ee369",
          "id": 778
        }
      },
      {
        "name": "TypeScript",
        "condition": "TypeScriptNuGet",
        "additionalItemTypes": [
          "TypeScriptCompile"
        ]
      }
    ],
    "projectIconProviders": {
      "cpsProjectIconSourceService": {
        "service": "Microsoft.VisualStudio.CpsProjectIconSourceService",
        "version": "0.1"
      }
    },
    "treeProviders": {
      "cpsProjectTreeProvider": {
        "type": "Project",
        "service": "Microsoft.VisualStudio.ProjectSystem.ProjectTreeProvider",
        "version": "0.1"
      }
    }
  },
  "consumes": {
    "projectSystems": [
      {
        "projectTypes": [
          "UnitTestProject"
        ],
        "data": [
          {
            "$properties": [
              "Name",
              "Path",
              "ActiveConfigurations"
            ],
            "ActiveConfigurations": {
              "$properties": [
                "Name",
                "OutputGroups"
              ],
              "OutputGroups": {
                "$filter": {
                  "OutputGroupsByName": [
                    [
                      "Built"
                    ]
                  ]
                },
                "$properties": [
                  "Name",
                  "Outputs"
                ],
                "Outputs": {
                  "$properties": [
                    "FinalOutputPath",
                    "IsKeyOutput"
                  ]
                }
              }
            }
          }
        ],
        "when": "open"
      }
    ]
  }
}
